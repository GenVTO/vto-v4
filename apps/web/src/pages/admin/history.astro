---
import AdminLayout from '../../layouts/AdminLayout.astro'
import { TryOnHistoryViewer } from '@/components/try-on/TryOnHistoryViewer'
---

<AdminLayout title="Try-On History" maxWidth="max-w-7xl">
  <div class="mb-8 text-center">
    <h1 class="font-display text-3xl font-bold tracking-tight">
      Try-On History
    </h1>
    <p class="text-muted-foreground mt-2">
      Visualize the history persisted in the database.
    </p>
  </div>

  <TryOnHistoryViewer client:load />
</AdminLayout>
