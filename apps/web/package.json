{
  "name": "@vto/web",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "cf:local": "astro build && wrangler dev --config wrangler.jsonc --env local",
    "cf:dev": "astro build && wrangler dev --config wrangler.jsonc --env development --remote",
    "cf:prod": "astro build && wrangler dev --config wrangler.jsonc --env production --remote",
    "cf:deploy:dev": "astro build && wrangler deploy --config wrangler.jsonc --env development",
    "cf:deploy:prod": "astro build && wrangler deploy --config wrangler.jsonc --env production",
    "cf:secrets:push": "node ./scripts/push-wrangler-secrets.mjs",
    "cf:secrets:push:dev": "node ./scripts/push-wrangler-secrets.mjs --env development",
    "cf:secrets:push:prod": "node ./scripts/push-wrangler-secrets.mjs --env production",
    "cf:secrets:dry-run": "node ./scripts/push-wrangler-secrets.mjs --env development --dry-run",
    "cf:secrets:dry-run:prod": "node ./scripts/push-wrangler-secrets.mjs --env production --dry-run",
    "type-check": "astro check",
    "test": "vitest run"
  },
  "dependencies": {
    "@astrojs/check": "0.9.6",
    "@astrojs/cloudflare": "12.6.12",
    "@astrojs/react": "4.4.2",
    "@hookform/resolvers": "5.2.2",
    "@radix-ui/react-slot": "1.2.4",
    "@vto/db": "workspace:*",
    "@vto/env": "workspace:*",
    "@vto/logger": "workspace:*",
    "@vto/storage": "workspace:*",
    "@vto/try-on": "workspace:*",
    "@vto/types": "workspace:*",
    "astro": "5.17.3",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-react": "0.575.0",
    "radix-ui": "1.4.3",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-hook-form": "7.71.2",
    "tailwind-merge": "3.5.0",
    "tailwindcss": "4.2.1",
    "tw-animate-css": "1.4.0",
    "zod": "4.3.6",
    "zod-form-data": "3.0.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "4.2.1",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@testing-library/user-event": "14.6.1",
    "happy-dom": "20.7.0",
    "prettier": "3.8.1",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.7.2"
  }
}
