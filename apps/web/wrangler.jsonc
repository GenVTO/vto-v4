      "routes": [
        {
          "pattern": "development-env.genvto.com",
          "custom_domain": true,
        },
      ],
      "vars": {
        "NODE_ENV": "development",
        "R2_CONFIGS_JSON": "[{\"name\":\"cloudflare\",\"binding\":\"TRYON_ASSETS\",\"bucketName\":\"vto-tryon-dev\",\"publicBaseUrl\":\"https://dev-img.genvto.com\"}]",
