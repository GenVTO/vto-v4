        "LOG_LEVEL": "debug",
        "LOG_ENABLE_PINO": "true",
        "LOG_ENABLE_SENTRY": "false",
        "LOG_ENABLE_AXIOM": "false",
        "AXIOM_DATASET": "api-dev",
        "TRYON_PROVIDER_ORDER": "fashn",
        "SENTRY_DSN": "https://988220f135b530c34e02936a596041a6@o4507666244698112.ingest.de.sentry.io/4508846995669072",
      },
      "r2_buckets": [
        {
          "binding": "TRYON_ASSETS",
          "bucket_name": "vto-tryon-dev",
        },
      ],
      "kv_namespaces": [
        {
          "binding": "SESSION",
          "id": "0b422db1074c417e9e5f3d69ff5e7a00",
          "preview_id": "c7ce4fc0208b470aa41450dc8d635776",
        },
      ],
    },
    "production": {
      "name": "vto-web-prod",
      "workers_dev": true,
      "routes": [
        {
          "pattern": "www.genvto.com",
          "custom_domain": true,
        },
        {
          "pattern": "genvto.com",
          "custom_domain": true,
        },
      ],
      "vars": {
        "NODE_ENV": "production",
        "R2_CONFIGS_JSON": "[{\"name\":\"cloudflare\",\"binding\":\"TRYON_ASSETS\",\"bucketName\":\"vto-tryon-prod\",\"publicBaseUrl\":\"https://img.genvto.com\"}]",
        "STORAGE_PROVIDER_ORDER": "r2:cloudflare",
