      "name": "vto-web-local",
      "workers_dev": true,
      "vars": {
        "NODE_ENV": "development",
        "R2_CONFIGS_JSON": "[]",
        "STORAGE_PROVIDER_ORDER": "s3:local,disk",
        "STORAGE_PUBLIC_URL_OVERRIDE": "https://s3-supa-dev.genvto.com",
        "LOG_LEVEL": "debug",
        "LOG_ENABLE_PINO": "true",
        "LOG_ENABLE_SENTRY": "false",
        "LOG_ENABLE_AXIOM": "false",
        "AXIOM_DATASET": "api-local",
        "TRYON_PROVIDER_ORDER": "fashn",
        "SENTRY_DSN": "https://988220f135b530c34e02936a596041a6@o4507666244698112.ingest.de.sentry.io/4508846995669072",
      },
      "kv_namespaces": [
        {
          "binding": "SESSION",
          "id": "362d9d36645d4bf38b909eeaf2016a3a",
          "preview_id": "ff1f5e0024434a6590cb7cba95797b3c",
        },
      ],
    },
    "development": {
      "name": "vto-web-dev",
