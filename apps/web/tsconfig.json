{
  "extends": ["../../tsconfig.base.json", "astro/tsconfigs/strict"],
  "include": [".astro/types.d.ts", "src", "astro.config.ts"],
  "exclude": ["dist"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "@/*": ["./src/*"]
    },
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ]
  }
}
